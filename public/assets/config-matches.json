{
  "heading": "Crescendo - 2024",
  "logo": "logo.png",
  "pages": [
    {
      "name": "Setup",
      "widgets": [
        {
          "name": "Scouter Name",
          "type": "text",
          "validation": {
            "comparison": "greater",
            "value": 0
          }
        },
        {
          "name": "Starting Position",
          "type": "positions",
          "file": "field.png"
        }
      ]
    },
    {
      "name": "Autonomous",
      "widgets": [
        {
          "name": "Left Starting Zone",
          "type": "checkbox",
          "colspan": 4
        },
        {
          "type": "spacer",
          "height": 20
        },
        {
          "name": "Amp Notes Made",
          "type": "spinbox",
          "prefix": "Auto",
          "row": 3,
          "col":1
        },
        {
          "name": "Amp Notes Missed",
          "type": "spinbox",
          "prefix": "Auto",
          "row": 3,
          "col":3
        },
        {
          "name": "Speaker Notes Made",
          "type": "spinbox",
          "prefix": "Auto",
          "row": 4,
          "col":1
        },
        {
          "name": "Speaker Notes Missed",
          "type": "spinbox",
          "prefix": "Auto",
          "row": 4,
          "col":3
        },
        {
        
          "type": "spacer",
          "height": 20
          
        },
        {
          "name": "Notes Picked Up",
          "type": "heading",
          "colspan": 5,
          "align": "center"
        },
        {
          "type": "picture",
          "file": "fieldLabeled.png",
          "colspan": 5,
          "align": "center"
        },
        {
          "name": "Use the labels in the above picture to identify notes used in auto.",
          "type": "label",
          "colspan": 5,
          "align": "center"
        },
        
        {
          "name": "Notes Picked Up In Auto",
          "type": "multicheckbox",
          "options": [
            "A",
            "B",
            "C",
            "D",
            "E",
            "F",
            "G",
            "H"
          ],
          "colspan": 5,
          "align": "center",
          "noLabel": true
        },
        {
          "name": "Auto Comments",
          "type": "textarea",
          "colspan": 5,
          "align": "center"
        }
      ]
    },
    {
      "name": "Teleop",
      "widgets": [
        {
          "name": "Picked Up From",
          "type": "heading",
          "colspan": 4,
          "align": "center"
        },
        {
          "name": "How do did they acquire notes? Source means the note slid into their robot from the source",
          "type": "label",
          "colspan": 4,
          "align": "center"
        },
        {
          "name": "Note Pickup Capabilities",
          "type": "dropdown",
          "options": [
            "Ground Only",
            "Source Only",
            "Both, prefers Ground",
            "Both, no preference",
            "No Pickup"
          ],
          "col": 2
        },
        {
          "name": "Scoring",
          "type": "heading",
          "colspan": 4,
          "align": "center"
        },
        {
          "name": "Speaker Notes",
          "type": "spinbox"
        },
        {
          "name": "Amp Notes",
          "type": "spinbox"
        },
        {
          "name": "Trap Notes",
          "type": "spinbox",
          "max": 3,
          "row": 5,
          "col": 3
        },
        {
          "type": "spacer",
          "height": 20
        },
        {
          "name": "Defense Rating",
          "type": "dropdown",
          "options": [
            "0 (Not Played)",
            "1",
            "2",
            "3",
            "4",
            "5 (Very Effective)"
          ],
          "col": 2
        }
      ]
    },
    {
      "name": "Endgame",
      "widgets": [
        {
          "name": "End Location",
          "type": "radio",
          "options": [
            "Onstage",
            "Parked",
            "None"
          ],
          "default": 2
        },
        {
          "name": "Failed Climb",
          "type": "checkbox"
        },
        {
          "name": "Harmony",
          "type": "checkbox"
        },
        {
          "name": "Mechanical Failures",
          "type": "text"
        },
        {
          "name": "Comments",
          "type": "textarea"
        },
        {
          "name": "Stage Positions",
          "type": "heading",
          "colspan": 2,
          "align": "center"
        },
        {
          "type": "picture",
          "file": "stage.png",
          "colspan": 2,
          "height": 250
        }
      ]
    }
  ]
}
