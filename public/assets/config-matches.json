{
  "heading": "Reefscape - 2025",
  "logo": "logo.png",
  "forceQualifiers": true,
  "pages": [
    {
      "name": "Setup",
      "widgets": [
        {
          "name": "Scouter Name",
          "type": "text"
        },
        {
          "type": "picture",
          "file": "fieldAutoStarts.png",
          "align": "center",
          "changeImageOnFlip": true,
          "flipImage": "fieldAutoStartsFlip.png",
          "colspan": 5
        },
        
        {
          "name": "Use the labels in the above picture to choose the robot's starting position.",
          "type": "label",
          "colspan": 5,
          "align": "center"
        }
      ]
    },
    {
      "name": "Autonomous",
      "widgets": [
        {
          "name": "Fuel Scored",
          "type": "heading",
          "align": "center",
          "colspan": 2
        },
        {
          "name": "Fuel Passed",
          "type": "heading",
          "align": "center",
          "colspan": 2,
          "row": 1,
          "col": 4
        },
        {
          "name": "test",
          "type": "multicounter",
          "buttonColor": "grey",
          "intervals": [1,5,10],
          "showNegatives": true,
          "noLabel": true,
          "row": 2,
          "col": 1
        },
        {
          "type": "spacer",
          "row": 2,
          "col": 3
        },
        {
          "name": "passed",
          "type": "multicounter",
          "intervals": [1,5,10],
          "showNegatives": true,
          "noLabel": true,
          "row": 2,
          "col": 4
        }
      ]
    },
    {
      "name": "Teleop",
      "widgets": [

        {
          "name": "Did they cross the field to play defense",
          "type": "checkbox",
          "col": 1,
          "row": 1,
          "colspan": 1,
          "labelColspan": 3
        },
        {
          "type": "spacer",
          "height": 20
        },
        {
          "type": "picture",
          "file": "coral.png",
          "col": 2
        },
        {
          "name": "L4 Coral",
          "type": "spinbox",
          "col": 1,
          "buttonColor": "white",
          "buttonTextColor": "black"
        },
        {
          "name": "L3 Coral",
          "type": "spinbox",
          "buttonColor": "white",
          "buttonTextColor": "black"
        },
        {
          "name": "L2 Coral",
          "type": "spinbox",
          "buttonColor": "white",
          "buttonTextColor": "black"
        },
        {
          "name": "L1 Coral",
          "type": "spinbox",
          "buttonColor": "white",
          "buttonTextColor": "black"
        }

      ]
    },
    {
      "name": "Endgame",
      "widgets": [
        {
          "name": "End Location",
          "type": "radio",
          "options": [
            "Hanging From Deep Cage",
            "Hanging From Shallow Cage",
            "Parked",
            "None"
          ],
          "default": 3
        },
        {
          "name": "Failed Climb",
          "type": "checkbox"
        },
        {
          "name": "Comments/MechanicalFailures",
          "type": "textarea"
          
        },
        {
          "name": "Robot Broke",
          "type": "checkbox"
        },
        {
          "name": "Defense Rating",
          "type": "dropdown",
          "options": [
            "0 (Not Played)",
            "1",
            "2",
            "3",
            "4",
            "5"
          ]
        }
        
      ]
    }
  ]
}
