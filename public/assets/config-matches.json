{
  "heading": "Rebuilt - 2026",
  "logo": "logo.png",
  "forceQualifiers": true,
  "pages": [
    {
      "name": "Setup",
      "widgets": [
        {
          "name": "Scouter Name",
          "type": "text",
          "validation": {
            "comparison": "greater",
            "value": 0
          }
        },
        {
          "type": "picture",
          "file": "field.png",
          "align": "center",
          "changeImageOnFlip": true,
          "flipImage": "field.png",
          "colspan": 5
        },
        
        {
          "name": "Use the labels in the above picture to choose the robot's starting position.",
          "type": "label",
          "colspan": 5,
          "align": "center"
        },
        {
          "name": "Starting Location",
          "type": "dropdown",
          "align": "center",
          "colspan": 5,
          "noLabel": true,
          "defaultOption": true,
          "validation": {
            "comparison": "greaterOrEqual",
            "value": 0
          },
          "options": [
            "1",
            "2",
            "3",
            "4",
            "5"
          ]
        }
      ]
    },
    {
      "name": "Autonomous",
      "widgets": [
        {
          "name": "Fuel Scored",
          "type": "heading",
          "align": "center",
          "colspan": 2
        },
        {
          "name": "Fuel Passed",
          "type": "heading",
          "align": "center",
          "colspan": 2,
          "row": 1,
          "col": 4
        },
        {
          "name": "Fuel Scored",
          "type": "multicounter",
          "buttonColor": "grey",
          "intervals": [1,5,10],
          "showNegatives": true,
          "noLabel": true,
          "row": 2,
          "col": 1,
          "colspan": 2,
          "prefix": "Auto"
        },
        {
          "name": "Fuel Passed",
          "type": "multicounter",
          "intervals": [1,5,10],
          "showNegatives": true,
          "noLabel": true,
          "colspan": 2,
          "row": 2,
          "col": 4,
          "prefix": "Auto"
        },
        {
          "name": "L1 Climb in Auto",
          "type": "checkbox",
          "row": 5
        },
        {
          "name": "Entered Neutral Zone",
          "type": "dropdown",
          "options": [
            "No",
            "Through Trench",
            "Over the Bump",
            "Both"
          ],
          "row": 5,
          "colspan": 2,
          "col": 3,
          "prefix": "Auto",
          "stringValues": true,
          "defaultOption": true,
          "defaultValue": "No"
        },

        {
          "name": "Failed Climb",
          "type": "checkbox",
          "prefix": "Auto"
        }
      ]
    },
    {
      "name": "Teleop",
      "widgets": [
        {
          "name": "Fuel Scored",
          "type": "heading",
          "align": "center",
          "colspan": 2
        },
        {
          "name": "Fuel Passed",
          "type": "heading",
          "align": "center",
          "colspan": 2,
          "row": 1,
          "col": 4
        },
        {
          "name": "Fuel Scored",
          "type": "multicounter",
          "buttonColor": "grey",
          "intervals": [1,5,10],
          "showNegatives": true,
          "noLabel": true,
          "row": 2,
          "col": 1,
          "colspan": 2
        },
        {
          "name": "Fuel Passed",
          "type": "multicounter",
          "intervals": [1,5,10],
          "showNegatives": true,
          "noLabel": true,
          "colspan": 2,
          "row": 2,
          "col": 4
        },
        {
          "name": "Entered Neutral Zone",
          "type": "dropdown",
          "options": [
            "No",
            "Through Trench",
            "Over the Bump",
            "Both"
          ],
          "row": 5,
          "colspan": 2,
          "col": 3,
          "stringValues": true
        }
      ]
    },
    {
      "name": "Endgame",
      "widgets": [
        {
          "type": "heading",
          "name": "What Roles did the robot play during the phases of the game?",
          "colspan": 4,
          "align": "center"
        },
        {
          "name": "Active Phase Roles",
          "type": "multicheckbox",
          "options": [
            "Clean-Up",
            "Passer",
            "Defender",
            "Cycler",
            "Thief"
          ],
          "row": 2,
          "colspan": 2,
          "stringValues": true
        },
        {
          "name": "Inactive Phase Roles",
          "type": "multicheckbox",
          "options": [
            "Clean-Up",
            "Passer",
            "Defender",
            "Cycler",
            "Thief"
          ],
          "row": 2,
          "col": 2,
          "colspan": 2,
          "stringValues": true

        },
        {
          "name": "End Location",
          "type": "radio",
          "options": [
            "None",
            "L1",
            "L2",
            "L3"
          ],
          "default": 0,
          "row": 4
        },
        {
          "name": "Failed Climb",
          "type": "checkbox"
        },
        {
          "name": "Comments/MechanicalFailures",
          "type": "textarea"
          
        },
        {
          "name": "Robot Broke",
          "type": "checkbox"
        },
        {
          "name": "Defense Rating",
          "type": "dropdown",
          "options": [
            "0 (Not Played)",
            "1",
            "2",
            "3",
            "4",
            "5"
          ]
        }
        
      ]
    }
  ]
}
